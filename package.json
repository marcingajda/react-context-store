{
  "name": "react-hooked-store",
  "version": "0.1.0",
  "description": "Library for storing global states using Hook and Context API",
  "keywords": [
    "context",
    "hooks",
    "react",
    "reducer",
    "state"
  ],
  "repository": "git@github.com:marcingajda/react-hooked-store.git",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc --p ./tsconfig.json ",
    "build:watch": "tsc --p ./tsconfig.json -w",
    "example": "webpack-dev-server --watch --config example/webpack.config.ts",
    "lint": "eslint --fix --ext .js,.ts,.tsx ./src",
    "lint:test": "eslint --fix --ext .js,.ts,.tsx ./test",
    "test": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:mutate": "stryker run"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "typescript": "^3.4.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-syntax-decorators": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@stryker-mutator/core": "^1.3.1",
    "@stryker-mutator/html-reporter": "^1.3.1",
    "@stryker-mutator/jest-runner": "^1.3.1",
    "@stryker-mutator/typescript": "^1.3.1",
    "@types/classnames": "^2.2.7",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^24.0.12",
    "@types/node": "^11.13.6",
    "@types/react": "16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/webpack": "^4.4.27",
    "@types/webpack-dev-server": "^3.1.5",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.6",
    "concurrently": "^4.1.0",
    "coveralls": "^3.0.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "jsx-loader": "^0.13.2",
    "node-sass": "^7.0.0",
    "prettier": "^1.17.0",
    "prop-types": "^15.7.2",
    "react-hooks-testing-library": "^0.5.0",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^7.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  }
}
